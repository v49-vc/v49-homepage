{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"module": "ES2022",
		"moduleResolution": "Node",
		"target": "ES2022",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"]
		},
		"useUnknownInCatchVariables": true,
		"strict": true,
		"allowSyntheticDefaultImports": true
	}
}
